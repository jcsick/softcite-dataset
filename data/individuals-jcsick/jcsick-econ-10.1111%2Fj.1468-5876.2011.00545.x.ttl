# https://github.com/howisonlab/softcite-pdf-files/blob/master/docs/pdf-files/economics_pdf_files/10.1111%252Fj.1468-5876.2011.00545.x.pdf
# also https://doi.org/10.1111%2Fj.1468-5876.2011.00545.x
doi:10.1111%2Fj.1468-5876.2011.00545.x rdf:type bioj:article ;
    rdf:type bioj:econ_article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:codable ;
                           citec:isPresent true; # true if can code
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:standard_type ;
                           citec:isPresent true ; # true if standard article
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention doi:10.1111%2Fj.1468-5876.2011.00545.x_JS01 ;
    # create name for in_text_mention like
    # doi:10.1111%2Fj.1468-5876.2011.00545.x_JH01

    # citations like:
    # doi-cited:10.1111%2Fj.1468-5876.2011.00545.x_AuthorYear
.

doi:10.1111%2Fj.1468-5876.2011.00545.x_JS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The  computational  results  are  obtained  by  using  Ox  version  5.10(Doornik, 2007).""" ; # use triple quotes

    citec:on_pdf_page 1  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 7 ; # integer
                           ca:memo """text implies use of Ox as statistical software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Ox""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent true ; # true/false
                           rdfs:label """5.10""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    citec:has_reference doi-cited:10.1111%2Fj.1468-5876.2011.00545.x_Doornik-2007 ; # name reference like doi-cited:a2004-40-NAT_GENET_Author-YYYY, no quotes
.

doi-cited:10.1111%2Fj.1468-5876.2011.00545.x_Doornik-2007 rdf:type citec:reference ;
    citec:full_quote """Doornik,  J. A.  (2007)Object-Oriented Matrix Programming Using Ox,  3rd  edn.,  London:  Timberlake  Con-sultants Press and Oxford.""" ; # use triple quotes

    citec:on_pdf_page 22  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:reference_type ;
                           rdfs:label """publication""" ; # put one in triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Ox""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                           # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                           # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Doornik,  J. A.""" ; # use triple quotes
                         ] ;
        ] ;
.
